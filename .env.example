# Database
DATABASE_URL=postgresql://analysis:analysis@postgres:5432/analysisdb

# External APIs (mock_first mode in v1)
SERP_API_KEY=your_serp_api_key_here
AHREFS_KEY=your_ahrefs_key_here
SEMRUSH_KEY=your_semrush_key_here

# Feature toggles
USE_MOCK_COLLECTORS=true
USE_SUPABASE=false

# Environment
ENV=development
LOG_LEVEL=INFO

# Security
ALLOWED_DOMAINS=genline.se,happycasino.se
COMPETITOR_DOMAINS=casinoguide.se,bettingexpert.se

# Rate limits
SERP_RATE_LIMIT=10
SERP_RATE_WINDOW=60
